<%= form_tag("/users", method: "post", remote: true) do %> 
    <%= label_tag 'username' %> 
        <%= text_field(:user, :username)%>
    </br>
    <%= label_tag "password" %> 
    </br>
    <%= text_field(:user, :password)%>
    </br>
    <%= label_tag "email" %> 
    </br>
    <%= text_field(:user, :email)%>
    </br>
    <%= submit_tag("Submit") %> 
    <% end %> 
