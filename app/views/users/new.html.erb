<form action="/users" method="post" accept-charset="UTF-8" data-remote="true">
<label for="username">Username:</label>
<input type="text" id="username" name="user[username]"><br><br>
<label for="password">Password:</label>
<input type="text" id="password" name="user[password]"><br><br>
<label for="email">Email:</label>
<input type="Email" id="email" name="user[email]"><br><br>
<input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
<input type="submit" value="Submit">
</form> 